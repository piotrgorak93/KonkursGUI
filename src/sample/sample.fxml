<VBox prefHeight="400.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="sample.GuiController">
    <children>
        <SplitPane dividerPositions="0.25059999999999993" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <items>
                <AnchorPane prefHeight="36.0" prefWidth="116.0">
                    <children>
                        <Label alignment="CENTER" layoutX="14.0" layoutY="14.0" minWidth="60.0" prefWidth="-1.0"
                               style="&#10;" text="" textAlignment="CENTER" wrapText="false">
                            <font>
                                <Font size="18.0" fx:id="x1"/>
                            </font>
                            <textFill>
                                <Color blue="0.624" green="0.624" red="0.624" fx:id="x2"/>
                            </textFill>
                        </Label>
                        <ComboBox fx:id="combo" layoutX="14.0" layoutY="55.0" prefWidth="150.0"
                                  promptText="Wybierz tablicę">
                            <items>
                                <FXCollections fx:id="list" fx:factory="observableArrayList">
                                    <String fx:id="tabelaA" fx:value="Tabela A"/>
                                    <String fx:id="tabelaB" fx:value="Tabela B"/>
                                    <String fx:id="tabelaC" fx:value="Tabela C"/>

                                </FXCollections>
                            </items>

                        </ComboBox>
                        <Label layoutX="14.0" layoutY="90.0" minWidth="60.0" prefWidth="-1.0" style="&#10;" text="Lub"
                               wrapText="false">
                            <font>
                                <Font size="18.0" fx:id="x11"/>
                            </font>
                            <textFill>
                                <Color blue="0.624" green="0.624" red="0.624" fx:id="x21"/>
                            </textFill>
                        </Label>
                        <ComboBox fx:id="combo1" layoutX="14.0" layoutY="126.0" prefWidth="150.0"
