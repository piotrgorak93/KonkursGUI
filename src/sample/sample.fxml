                    </children>
                </AnchorPane>
                <ScrollPane prefHeight="-1.0" prefWidth="-1.0">
                    <content>
                        <AnchorPane>
                            <children>
                                <Label alignment="CENTER" font="$x1" layoutX="14.0" layoutY="14.0" style="&#10;"
                                       text="View" textAlignment="CENTER" textFill="$x2" wrapText="false"/>
                                <AnchorPane layoutX="14.0" layoutY="50.0">
                                    <children>
                                        <TextArea fx:id="textarea" AnchorPane.bottomAnchor="0.0"
                                                  AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0"
                                                  AnchorPane.topAnchor="0.0">
                                        </TextArea>
                                    </children>
                                </AnchorPane>
                            </children>
                        </AnchorPane>
                    </content>
                </ScrollPane>
            </items>
        </SplitPane>

    </children>
</VBox>
